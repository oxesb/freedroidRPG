msgid "SADD - Exterminate Mode"
msgstr "DDAS – Mode extermination"

#: data/base/npc_specs.lua:28
msgid "SADD - Secret Area Defense Droid"
msgstr "DDAS - Droïde de Défense de  l'Aire Secrète"

#: data/base/npc_specs.lua:30
msgid "Richard - Programmer"
msgstr "Richard - Programmeur"

#: data/base/npc_specs.lua:32
msgid "Fighter"
msgstr "Combattant"

#: data/base/npc_specs.lua:32
msgid "Pendragon - Fighter"
msgstr "Pendragon - Combattant"

#: data/base/npc_specs.lua:39
msgid "Michelangelo - Chef"
msgstr "Michelangelo - Chef cuistot"

#: data/base/npc_specs.lua:43 data/storyline/act1/ReturnOfTux.droids:338
msgid "Koan"
msgstr "Koan"

#: data/base/npc_specs.lua:45
msgid "614 - Kevin's Guard"
