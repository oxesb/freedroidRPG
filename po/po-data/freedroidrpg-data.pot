msgstr ""

#: data/base/droid_archetypes.dat:987
msgid "Woman"
msgstr ""

#: data/base/droid_archetypes.dat:1005
msgid "This is Tania. She is human."
msgstr ""

#: data/base/droid_archetypes.dat:1009
msgid "Bartender"
msgstr ""

#: data/base/droid_archetypes.dat:1027
msgid "This is the bartender. Care for a drink?"
msgstr ""

#: data/base/droid_archetypes.dat:1031
