msgstr "добро"

#: data/storyline/act1/dialogs/c-net-nethack_sub.lua:297
msgid "a chaotic"
msgstr "хаос"

#: data/storyline/act1/dialogs/c-net-nethack_sub.lua:298
#: data/storyline/act1/dialogs/c-net-nethack_sub.lua:783
