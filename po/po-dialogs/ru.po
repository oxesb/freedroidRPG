msgstr "[b]Загружается[/b] Планы_Дворака.lua"

#: data/storyline/act2/dialogs/TerminalColemak.lua:133
msgid "[b]...Done.[/b]"
msgstr "[b]...Выполнено.[/b]"

#: data/storyline/act2/dialogs/TerminalColemak.lua:135
