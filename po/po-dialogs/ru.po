msgid "Francis"
msgstr "Фрэнсис"

#. TRANSLATORS: Ha! Your soul is mine, Franics!
#: data/storyline/act1/dialogs/Sorenson.lua:211
msgid "H4! Y0ur s0ul 15 min3, Francis!"
msgstr "Х4! 780я 9уш4 м0я, Фрэнсис!"

