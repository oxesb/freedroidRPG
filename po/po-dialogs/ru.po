#, c-format
msgid "You notice a [b]%s[/b] in front of you."
msgstr "Ты заметил, что перед тобой [b]%s[/b]."

#. TRANSLATORS: %s = enemy name
#: data/storyline/act1/dialogs/c-net-nethack_sub.lua:470
#, c-format
